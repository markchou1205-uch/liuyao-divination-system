{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/divination",
      "dest": "/pages/divination.html"
    },
    {
      "src": "/admin",
      "dest": "/admin/admin.html"
    },
    {
      "src": "/(.*)",
      "dest": "/pages/index.html"
    }
  ]
}
